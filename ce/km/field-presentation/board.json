{
  "$id": "ce_board",
  "$schema": "https://schema.yext.com/config/km/field-presentation/v1",
  "entityType": "ce_knowledgeboard",
  "displayName": "Board",
  "presentationEntries": [
    {
      "displayName": "Content",
      "entryType": "DIVIDER"
    },
    {
      "displayName": "Core Information",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "name",
          "visibleOnAdd": true
        },
        {
          "id": "entityId",
          "visibleOnAdd": true
        },
        {
          "id": "c_ownerEmail",
          "visibleOnAdd": true
        },
        {
          "id": "c_boardParentFolder",
          "visibleOnAdd": true
        },
        {
          "id": "c_cards",
          "visibleOnAdd": true
        },
        {
          "id": "c_tags",
          "visibleOnAdd": true
        },
        {
          "id": "c_status",
          "visibleOnAdd": true
        }
      ]
    },
    {
      "displayName": "Organizational Information",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "timezone",
          "visibleOnAdd": true
        },
        {
          "id": "slug",
          "visibleOnAdd": true
        }
      ]
    },
    {
      "displayName": "Other",
      "entryType": "DIVIDER"
    },
    {
      "displayName": "Internal Use Only",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "folder"
        },
        {
          "id": "labels"
        }
      ]
    }
  ]
}